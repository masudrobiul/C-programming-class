#include <stdio.h>
int main()
{
    //Write a C program to check whether a character is an alphabet or not.
    char alp;
    scanf("%c", &alp);

    if ((alp >= 'a' && alp <= 'z') || (alp >= 'A' && alp <= 'Z'))
    {
        printf("%c is an alphabet", alp);
    }
    else
    {
        printf("%c is not an alphabet", alp);
    }
    return 0;
}
