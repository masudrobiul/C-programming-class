#include <stdio.h>
int main()
{
    //Write a C program to find the maximum between three numbers.
    int a, b, c;
    scanf("%d %d %d", &a, &b, &c);
    if(a>b)
    {
        printf("a is max");
    }
    else if (b>c)
    {
        printf("b is max");
    }
    else if (c>a)
    {
        printf("c is max");
    }
    else
    {
        printf("all are equal");
    }
    return 0;
}
