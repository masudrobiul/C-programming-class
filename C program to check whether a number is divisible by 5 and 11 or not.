#include <stdio.h>
int main()
{
    //Write a C program to check whether a number is divisible by 5 and 11 or not.
    int a;
    scanf("%d", &a);
    if ((a%11==0) && (a%5==0))
    {
        printf("The number is divisable by 11 & 5\n");
    }
    else
    {
        printf("The number not divisable by 5 or 11");
    }
    return 0;

}
